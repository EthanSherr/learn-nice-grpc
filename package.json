  "main": "index.js",
  "scripts": {
    "app": "ts-node ./app.ts",
    "client": "ts-node ./client.ts",
    "asyncit": "ts-node ./asyncit.ts",
    "example-protos": "pnpm grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./compiled_proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=true,outputPartialMethods=false --proto_path=./proto   ./proto/example.proto",
    "book-protos": "pnpm grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./compiled_proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=true,outputPartialMethods=false --proto_path=./proto   ./proto/book.proto",
    "filestream-protos": "pnpm grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./compiled_proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=true,outputPartialMethods=false --proto_path=./proto   ./proto/filestream.proto"
  },
  "keywords": [],
